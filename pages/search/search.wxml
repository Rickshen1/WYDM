<!--pages/search/search.wxml-->
<view class="search page_row">  
  <input placeholder="请输入你要搜索的内容" bindinput="getSearchInput" />  
  <button bindtap="searchItem">search</button>  
  
</view>  
<view class="search_no" wx:if="{{!content_show}}">  
  <text>很抱歉,没有找到您要搜索的资料/(ㄒoㄒ)/~~</text>  
</view>  
<scroll-view style="height:300px" scroll-y="true" bindscrolltolower="scrollToLowerHandler">
        <block wx:for="{{resJsonData}}" wx:key="{{cgi}}">
           <view class="tr bg-g" id="{{item.cgi}}" bindtap="itemClick" wx:if="{{index %2 == 0}}">{{item.cellname}}</view>
           <view class="tr" id="{{item.cgi}}" bindtap="itemClick" wx:else>{{item.cellname}}</view>
        </block>
    </scroll-view>
